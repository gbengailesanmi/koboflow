{
  "name": "money-mapper-monorepo",
  "version": "1.0.0",
  "private": true,
  "packageManager": "yarn@1.22.22",
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev --parallel",
    "lint": "turbo run lint",
    "clean": "rm -rf node_modules packages/*/node_modules packages/*/.next packages/*/dist"
  },
  "devDependencies": {
    "turbo": "^2.7.2",
    "typescript": "^5.3.3"
  },
  "resolutions": {
    "next": "^16.1.1",
    "react": "19.2.3",
    "react-dom": "19.2.3"
  }
}
